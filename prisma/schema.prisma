generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Blog {
    id          String   @id @default(cuid())
    image       String
    title       String
    content     String
    description String
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt

    @@map("blogs")
}
